fun main(){
  ex5
}

fun ex5(){
    print("Digite sua nota: ")
    val n1:Double = readln().toDouble()
    print("Digite outra nota: ")
    val n2:Double = readln().toDouble()
    val media:Double = (n1 + n2) / 2
    println("Nota 1: ${n1}")
    println("Nota 1: ${n2}")
    print("A média entre ${n1} e ${n2} é igual ${media}")
}
